{
  "extends": "aegir/src/config/tsconfig.aegir.json",
  "compilerOptions": {
    "baseUrl": ".",
    "incremental": true,
    "composite": true,
    "sourceRoot": "./",
    "outDir": "../dist",
    "emitDeclarationOnly": false,
    "module": "ES2020",
    "target": "ES2020",
    "paths": {
      "generated": ["../generated/fetch/src/index.ts"],
      "generated/*": ["../generated/fetch/src/*"],
    }
  },
  "references": [
    {
      "path": "../tsconfig.generated.json",
    },
  ],
  "include": [
    "./index.ts",
  ]
}
