{
  "compilerOptions": {
    "composite": true,
    "incremental": true,
    "sourceRoot": "./generated/fetch/src",
    "outDir": "./dist/generated/fetch/src",
    "emitDeclarationOnly": false,
    "module": "CommonJS",
    "target": "ES2020",
    "typeRoots": [
      "./types",
      "./node_modules/@types"
    ],
    "declaration": true,
    "noEmitOnError": true,
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "isolatedModules": false,
    "rootDir": "./generated/fetch/src",
    "sourceMap": true,
    "moduleResolution":"Node"

  },
  "include": [
    "./generated/fetch/src",
    "./types/global.d.ts",
  ],
  "exclude": [
    "./src"
  ]
}
